<mvc:View controllerName="com.mscg.applications.cubiscan.approval.controller.Worklist" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"                                                                                                                               
	xmlns:semantic="sap.f.semantic" xmlns:footerbar="sap.ushell.ui.footerbar">                                                                                                                                                                                    
	<semantic:SemanticPage id="page" headerPinnable="false" toggleHeaderOnTitleClick="false">                                                                                                                                                                     
		<semantic:titleHeading>                                                                                                                                                                                                                                      
			<Title text="{i18n>worklistTitle}"/>                                                                                                                                                                                                                        
		</semantic:titleHeading>                                                                                                                                                                                                                                     
		<semantic:content>                                                                                                                                                                                                                                           
			<Table id="table" width="auto" noDataText="{worklistView>/tableNoDataText}" busyIndicatorDelay="{worklistView>/tableBusyDelay}"                                                                                                                             
				updateFinished="onUpdateFinished" mode="MultiSelect">                                                                                                                                                                                                      
				<headerToolbar>                                                                                                                                                                                                                                            
					<Toolbar>                                                                                                                                                                                                                                                 
						<Title id="tableHeader" text="{worklistView>/worklistTableTitle}"/>                                                                                                                                                                                      
						<ToolbarSpacer/>                                                                                                                                                                                                                                         
						<Input id="warehouseNumberInput" type="Text" showValueHelp="true" valueHelpOnly="true" tooltip="Warehouse Number" width="120px"                                                                                                                          
							valueHelpRequest="onWarehouseNumberValueHelpRequest" value="{fioriConfiguration>/user/defaults/warehouseNumber}" change="onWarehouseNumberChange"></Input>                                                                                              
						<Button id="approveButton" icon="sap-icon://accept" tooltip="Approve" press="onApproveItems"/>                                                                                                                                                           
						<Button id="rejectButton" icon="sap-icon://decline" tooltip="Reject" press="onRejectItems"/>                                                                                                                                                             
					</Toolbar>                                                                                                                                                                                                                                                
				</headerToolbar>                                                                                                                                                                                                                                           
				<columns>                                                                                                                                                                                                                                                  
					<Column>                                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.product}"/>                                                                                                                                                                                                           
					</Column>                                                                                                                                                                                                                                                 
					<Column hAlign="Center">                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.unitOfMeasure}"/>                                                                                                                                                                                                     
					</Column>                                                                                                                                                                                                                                                 
					<Column/>                                                                                                                                                                                                                                                 
					<Column hAlign="Center">                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.length}"/>                                                                                                                                                                                                            
					</Column>                                                                                                                                                                                                                                                 
					<Column hAlign="Center">                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.lengthOld}"/>                                                                                                                                                                                                         
					</Column>                                                                                                                                                                                                                                                 
					<Column hAlign="Center">                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.width}"/>                                                                                                                                                                                                             
					</Column>                                                                                                                                                                                                                                                 
					<Column hAlign="Center">                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.widthOld}"/>                                                                                                                                                                                                          
					</Column>                                                                                                                                                                                                                                                 
					<Column hAlign="Center">                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.height}"/>                                                                                                                                                                                                            
					</Column>                                                                                                                                                                                                                                                 
					<Column hAlign="Center">                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.heightOld}"/>                                                                                                                                                                                                         
					</Column>                                                                                                                                                                                                                                                 
					<Column hAlign="Center">                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.dimensionUom}"/>                                                                                                                                                                                                      
					</Column>                                                                                                                                                                                                                                                 
					<Column hAlign="Center">                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.grossWeight}"/>                                                                                                                                                                                                       
					</Column>                                                                                                                                                                                                                                                 
					<Column hAlign="Center">                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.grossWeightOld}"/>                                                                                                                                                                                                    
					</Column>                                                                                                                                                                                                                                                 
					<Column hAlign="Center">                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.weightUom}"/>                                                                                                                                                                                                         
					</Column>                                                                                                                                                                                                                                                 
					<Column hAlign="Center">                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.volume}"/>                                                                                                                                                                                                            
					</Column>                                                                                                                                                                                                                                                 
					<Column hAlign="Center">                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.volumeOld}"/>                                                                                                                                                                                                         
					</Column>                                                                                                                                                                                                                                                 
					<Column hAlign="Center">                                                                                                                                                                                                                                  
						<Text text="{i18n>worklist.columns.volumeUom}"/>                                                                                                                                                                                                         
					</Column>                                                                                                                                                                                                                                                 
				</columns>                                                                                                                                                                                                                                                 
			</Table>                                                                                                                                                                                                                                                    
		</semantic:content>                                                                                                                                                                                                                                          
		<semantic:sendEmailAction>                                                                                                                                                                                                                                   
			<semantic:SendEmailAction id="shareEmail" press="onShareEmailPress"/>                                                                                                                                                                                       
		</semantic:sendEmailAction>                                                                                                                                                                                                                                  
		<semantic:shareInJamAction>                                                                                                                                                                                                                                  
			<semantic:ShareInJamAction id="shareInJam" visible="{FLP>/isShareInJamActive}" press="onShareInJamPress"/>                                                                                                                                                  
		</semantic:shareInJamAction>                                                                                                                                                                                                                                 
		<semantic:saveAsTileAction>                                                                                                                                                                                                                                  
			<footerbar:AddBookmarkButton id="shareTile" title="{worklistView>/saveAsTileTitle}"/>                                                                                                                                                                       
		</semantic:saveAsTileAction>                                                                                                                                                                                                                                 
	</semantic:SemanticPage>                                                                                                                                                                                                                                      
</mvc:View>                                                                                                                                                                                                                                                    