<core:FragmentDefinition                                                                                                                                                                                                                                       
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:core="sap.ui.core">                                                                                                                                                                                                                                     
	<ViewSettingsDialog                                                                                                                                                                                                                                           
		confirm="handleViewSettingsDialogConfirm">                                                                                                                                                                                                                   
		<filterItems>                                                                                                                                                                                                                                                
			<ViewSettingsFilterItem                                                                                                                                                                                                                                     
				text="Status"                                                                                                                                                                                                                                              
				key="status"                                                                                                                                                                                                                                               
				multiSelect="false">                                                                                                                                                                                                                                       
				<items>                                                                                                                                                                                                                                                    
					<ViewSettingsItem text="New Item" key="status__EQ__N"/>                                                                                                                                                                                                   
					<ViewSettingsItem text="Scanned" key="status__EQ__S"/>                                                                                                                                                                                                    
					<ViewSettingsItem text="Updated to ECC" key="status__EQ__U"/>                                                                                                                                                                                             
					<ViewSettingsItem text="Re-Measure" key="status__EQ__R"/>                                                                                                                                                                                                 
					<ViewSettingsItem text="Approved but not Updated to ECC" key="status__EQ__A"/>                                                                                                                                                                            
				</items>                                                                                                                                                                                                                                                   
			</ViewSettingsFilterItem>                                                                                                                                                                                                                                   
		</filterItems>                                                                                                                                                                                                                                               
	</ViewSettingsDialog>                                                                                                                                                                                                                                         
</core:FragmentDefinition>                                                                                                                                                                                                                                     